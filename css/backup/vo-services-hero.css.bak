    /* Remove ALL orange decorations */
    .vo-therapist-banner *::before,
    .vo-therapist-banner *::after,
    .venting-banner-figma *::before,
    .venting-banner-figma *::after {
        display: none !important;
        content: none !important;
        background: none !important;
        border: none !important;
    }

    .vo-figma-container,
    .figma-container {
        gap: 35px !important;
    }

    .vo-graphics-wrapper,
    .graphics-wrapper {
        margin: 30px auto 35px !important;
        background: transparent !important;
    }

    .vo-image-frame,
    .image-frame {
        background: transparent !important;
        border: none !important;
        background: transparent !important;
    }

    /* Just the image, circular */
    .vo-banner-slideshow-inner,
    .banner-slideshow-inner {
        border-radius: 50% !important;
        overflow: hidden !important;
        background: transparent !important;
    }

    .vo-blob-slide.active,
    .blob-slide.active {
        border-radius: 50% !important;
    }
}

/* =========================================
   BANNER - REMOVE ALL GRAPHICS (ORANGE & BLACK)
   ========================================= */
@media (max-width: 768px) {

    /* Remove absolutely everything decorative */
    .vo-graphics-wrapper svg,
    .graphics-wrapper svg,
    .vo-graphics-wrapper canvas,
    .graphics-wrapper canvas,
    .vo-graphics-wrapper .blob,
    .graphics-wrapper .blob,
    .vo-graphics-wrapper>*:not(.vo-image-frame):not(.image-frame),
    .graphics-wrapper>*:not(.vo-image-frame):not(.image-frame) {
        display: none !important;
    }

    .vo-image-frame>*:not(.vo-banner-slideshow-inner):not(.banner-slideshow-inner),
    .image-frame>*:not(.vo-banner-slideshow-inner):not(.banner-slideshow-inner) {
        display: none !important;
    }

    /* Clean image only */
    .vo-graphics-wrapper,
    .graphics-wrapper {
        width: 200px !important;
        height: 200px !important;
        margin: 30px auto 40px !important;
        background: none !important;
        overflow: visible !important;
    }

    .vo-image-frame,
    .image-frame {
        width: 100% !important;
        height: 100% !important;
        background: none !important;
        border: none !important;
        box-shadow: none !important;
        overflow: hidden !important;
        border-radius: 50% !important;
    }

    .vo-banner-slideshow-inner,
    .banner-slideshow-inner {
        width: 100% !important;
        height: 100% !important;
        border-radius: 50% !important;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.2) !important;
    }
}

